<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.txWeb.spider.mapper.NewsMapper">

    <insert id="save">
        INSERT INTO news(title,intro,source,url,publish_time) VALUES(#{title},#{intro},#{source},#{url},#{publish_time})
    </insert>
</mapper>